	Number = {7},
	Pages = {1-16},
	Publisher = {Public Library of Science},
	Title = {Stimulating Contributions to Public Goods through Information Feedback: Some Experimental Results},
	Url = {http://dx.doi.org/10.1371%2Fjournal.pone.0159537},
	Volume = {11},
	Year = {2016},
